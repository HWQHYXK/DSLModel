<?xml version="1.0" encoding="utf-8"?>
<EntityCollection>
  <Entity>
    <EntityCode>Order</EntityCode>
    <EntityName>销售订单</EntityName>
    <EntityDescription>销售订单模型</EntityDescription>
    <EntityNameSpace>Inspur.Gsp.Order</EntityNameSpace>
    <EntityTableCode>GspOrder</EntityTableCode>
    <EntityFieldCollection>
      <EntityField>
        <FieldCode>ID</FieldCode>
        <FieldName>内码</FieldName>
        <FieldType>string</FieldType>
        <FieldConstraint>
          <MaxLength>36</MaxLength>
          <MinLength>36</MinLength>
          <IsEmpty>false</IsEmpty>
        </FieldConstraint>
      </EntityField>
      <EntityField>
        <FieldCode>Code</FieldCode>
        <FieldName>订单编号</FieldName>
        <FieldType>string</FieldType>
        <FieldConstraint>
          <MaxLength>50</MaxLength>
          <MinLength>1</MinLength>
          <IsEmpty>false</IsEmpty>
        </FieldConstraint>
      </EntityField>
      <EntityField>
        <FieldCode>Name</FieldCode>
        <FieldName>订单名称</FieldName>
        <FieldType>string</FieldType>
        <FieldConstraint>
          <MaxLength>50</MaxLength>
          <MinLength>1</MinLength>
          <IsEmpty>false</IsEmpty>
        </FieldConstraint>
      </EntityField>
      <EntityField>
        <FieldCode>Price</FieldCode>
        <FieldName>单价</FieldName>
        <FieldType>double</FieldType>
        <FieldConstraint>
          <Length>8</Length>
          <Precision>2</Precision>
        </FieldConstraint>
      </EntityField>
      <EntityField>
        <FieldCode>OrderCount</FieldCode>
        <FieldName>订单数目</FieldName>
        <FieldType>int</FieldType>
      </EntityField>
      <EntityField>
        <FieldCode>OrderAmount</FieldCode>
        <FieldName>订单金额</FieldName>
        <FieldType>double</FieldType>
        <FieldConstraint>
          <Length>8</Length>
          <Precision>2</Precision>
        </FieldConstraint>
      </EntityField>
      <EntityField>
        <FieldCode>Status</FieldCode>
        <FieldName>订单状态</FieldName>
        <FieldType>int</FieldType>
        <FieldConstraint>
          <IsEnum>true</IsEnum>
          <EnumCollection>
            <Key>0</Key>
            <Value>Creating</Value>
          </EnumCollection>
          <EnumCollection>
            <Key>1</Key>
            <Value>Created</Value>
          </EnumCollection>
        </FieldConstraint>
      </EntityField>
      <EntityField>
        <FieldCode>IsVip</FieldCode>
        <FieldName>是否会员</FieldName>
        <FieldType>bool</FieldType>
      </EntityField>
      <EntityField>
        <FieldCode>CreateTime</FieldCode>
        <FieldName>创建时间</FieldName>
        <FieldType>DateTime</FieldType>
      </EntityField>
      <EntityField>
        <FieldCode>OrderItems</FieldCode>
        <FieldName>订单明细项</FieldName>
        <FieldType>List&lt;T&gt;</FieldType>
        <FieldConstraint>
          <T>OrderItem</T>
        </FieldConstraint>
      </EntityField>
    </EntityFieldCollection>
  </Entity>


  <Entity>
    <EntityCode>OrderItem</EntityCode>
    <EntityName>销售订单明细</EntityName>
    <EntityDescription>销售订单明细模型</EntityDescription>
    <EntityNameSpace>Inspur.Gsp.Order</EntityNameSpace>
    <EntityTableCode>GspOrderItem</EntityTableCode>
    <EntityFieldCollection>
      <EntityField>
        <FieldCode>ID</FieldCode>
        <FieldName>内码</FieldName>
        <FieldType>string</FieldType>
        <FieldConstraint>
          <MaxLength>36</MaxLength>
          <MinLength>36</MinLength>
          <IsEmpty>false</IsEmpty>
        </FieldConstraint>
      </EntityField>
      <EntityField>
        <FieldCode>OrderId</FieldCode>
        <FieldName>所属销售订单内码</FieldName>
        <FieldType>string</FieldType>
        <FieldConstraint>
          <MaxLength>36</MaxLength>
          <MinLength>36</MinLength>
          <IsEmpty>false</IsEmpty>
        </FieldConstraint>
      </EntityField>
      <EntityField>
        <FieldCode>ProductCode</FieldCode>
        <FieldName>产品编号</FieldName>
        <FieldType>string</FieldType>
        <FieldConstraint>
          <MaxLength>50</MaxLength>
          <MinLength>1</MinLength>
          <IsEmpty>false</IsEmpty>
        </FieldConstraint>
      </EntityField>
      <EntityField>
        <FieldCode>ProductName</FieldCode>
        <FieldName>产品名称</FieldName>
        <FieldType>string</FieldType>
        <FieldConstraint>
          <MaxLength>50</MaxLength>
          <MinLength>1</MinLength>
          <IsEmpty>false</IsEmpty>
        </FieldConstraint>
      </EntityField>
    </EntityFieldCollection>
  </Entity>
</EntityCollection>
